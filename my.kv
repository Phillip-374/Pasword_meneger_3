<MainEncryptWindow>:

    pasword: pasword
    picture_path: picture_path
    text: text


    name: "main_encrypt"
    canvas.before:
        Color:
            rgba: 52,52,52,0.2
        Rectangle:
            pos:self.pos
            size:self.size





    FloatLayout:


    Image
        pos_hint: {'top':1.47}
        source: 'widgets/top_regtangle_main.png'





    TextInput:
        id: picture_path
        hint_text:'Введите путь к файлу карттинки'
        background_color: (0.5, 0.5, 0.5, 1)
        foreground_color: (1, 1, 1, 1)
        pos_hint: {'x':0.1,'y':0.67}
        size_hint:0.75,0.2


    TextInput:
        id: text
        hint_text:'Введите текст'
        background_color: (0.5, 0.5, 0.5, 1)
        foreground_color: (1, 1, 1, 1)
        pos_hint: {'x':0.1,'y':0.57}
        size_hint:0.75,0.07


    Label:
        text: 'Пароль для шифрования'
        pos_hint: {'x':-0.05,'y':0.02}
        font_size:17
        background_color: 1,1,1,1

    TextInput:
        id: pasword
        hint_text:'Можно использовать только сгенерированые пароли'
        background_color: (0.5, 0.5, 0.5, 1)
        foreground_color: (1, 1, 1, 1)
        pos_hint: {'x':0.1,'y':0.37}
        size_hint:0.65,0.115

    #перегенерировать пароль
    Button:
        pos_hint: {'x':0.77,'y':0.4}
        size_hint: .12, .07
        background_color: 0,0,0,0



        on_release:
            root.regenerate_pasword()

        Image
            #size:160,200
            center_x: self.parent.center_x
            center_y: self.parent.center_y
            source: 'widgets/regenerate.png'


    #Зашифровать
    Button:
        font_size:10
        pos_hint: {'x':0.21,'y':0.25}
        size_hint: .57, .07
        background_color: 0,0,0,0

        on_release:
            root.encrypted()


        Image:
            source: 'widgets/button_encrypt_neon.png'
            size:160,200
            center_x: self.parent.center_x
            center_y: self.parent.center_y


    #закрытый замок
    Button:
        font_size:10
        pos_hint: {'x':0.05,'y':0.13}
        size_hint: .57, .07
        background_color: 0,0,0,0



        Image:
            source: 'widgets/closed_lock_neon.png'
            size:160,200
            center_x: self.parent.center_x
            center_y: self.parent.center_y


    #открытый замок
    Button:
        font_size:10
        pos_hint: {'x':0.38,'y':0.13}
        size_hint: .57, .07
        background_color: 0,0,0,0

        on_release:
            app.root.current = "main_decrypt"
            #root.add_pasword_in_grid()


        Image:
            source: 'widgets/open_lock.png'
            size:160,200
            center_x: self.parent.center_x
            center_y: self.parent.center_y



    #на главную
    Button:
        pos_hint: {'x':0.1,'y':0.015}
        size_hint: .1, .05
        background_color: 0,0,0,0



        Image:
            source: 'widgets/home_neon.png'
            center_x: self.parent.center_x
            center_y: self.parent.center_y


    #на список паролей
    Button:
        pos_hint: {'x':0.45,'y':0.015}
        size_hint: .1, .05
        background_color: 0,0,0,0

        on_release:
            app.root.current = "pasword_list"

        Image:
            source: 'widgets/pasword_list.png'
            center_x: self.parent.center_x
            center_y: self.parent.center_y


    #на настройки
    Button:
        pos_hint: {'x':0.8,'y':0.015}
        size_hint: .1, .05
        background_color: 0,0,0,0

        on_release:
            app.root.current = "settings"
            root.manager.transition.direction = "right"

        Image:
            source: 'widgets/settings.png'
            center_x: self.parent.center_x
            center_y: self.parent.center_y








